        \scr@ifundefinedorrelax{Gm@bindingoffset}{}{%
          \hskip\Gm@bindingoffset}%
      }{\hskip\ta@bcor}%
    \else
      \hskip-\evensidemargin
    \fi
    \vbox to\z@{\hsize\z@
      \vskip-\baselineskip
      \makebox[0pt][l]{\usebox\titlebox}%
      \vss
    }%
  \end{titlepage}
}
