%    \begin{macrocode}
\newsavebox\titlebox
\newenvironment{fullsizetitle}[1][]{%
  \TitleOptions{#1}%
  \begin{lrbox}{\titlebox}
    \hsize\paperwidth
    \scr@ifundefinedorrelax{ta@bcor}{%
      \scr@ifundefinedorrelax{Gm@bindingoffset}{}{%
        \advanve\hsize-\Gm@bindingoffset}%
    }{%
      \advance\hsize-\ta@bcor
      \scr@ifundefinedorrelax{Gm@bindingoffset}{}{%
